{
  "name": "sfdx-devops",
  "description": "SFDX plugin for DevOps actions related to Salesforce Development",
  "version": "0.1.2",
  "author": "Anthony Heber @aheber",
  "bugs": "https://github.com/aheber/sfdx-devops/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^2.2.3",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2",
    "deep-equal": "^2.0.1",
    "dir-compare": "^2.0.0",
    "eol": "^0.9.1",
    "fs-extra": "^8.1.0",
    "globby": "^11",
    "install": "^0.13.0",
    "js-yaml": "^3.13.1",
    "tslib": "^1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.5.0",
    "@types/chai": "^4",
    "@types/fs-extra": "^8.0.1",
    "@types/mocha": "^5",
    "@types/node": "^10.17.13",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin-tslint": "^2.20.0",
    "chai": "^4",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.10.0",
    "mocha": "^7",
    "nyc": "^15",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aheber/sfdx-devops",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "devops": {
        "description": "DevOps commands for working with Salesforce source code"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "aheber/sfdx-devops",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "eslint . --ext .ts",
    "version": "oclif-dev readme && git add README.md"
  }
}
