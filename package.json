{
  "name": "sfdx-devops",
  "description": "SFDX plugin for DevOps actions related to Salesforce Development",
  "version": "0.4.9",
  "author": "Anthony Heber @aheber",
  "bugs": "https://github.com/aheber/sfdx-devops/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^5.1.15",
    "@salesforce/command": "^5.2.13",
    "@salesforce/core": "^3.31.10",
    "@salesforce/plugin-org": "^2.2.9",
    "@salesforce/ts-sinon": "^1.2.3",
    "deep-equal": "^2.0.3",
    "deep-object-diff": "^1.1.0",
    "dir-compare": "^4.0.0",
    "eol": "^0.9.1",
    "fs-extra": "^10.1.0",
    "globby": "^11.0.0",
    "install": "^0.13.0",
    "js-yaml": "^4.1.0",
    "puppeteer": "^19.0.0",
    "sort-any": "^4.0.0",
    "tslib": "^2.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^2.2.4",
    "@salesforce/dev-config": "^3.1.0",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin-tslint": "^5.40.1",
    "chai": "^4",
    "eslint": "^8.25.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aheber/sfdx-devops",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "devops": {
        "description": "DevOps commands for working with Salesforce source code"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "aheber/sfdx-devops",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack1": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "eslint . --ext .ts",
    "version": "oclif-dev readme && git add README.md"
  }
}
